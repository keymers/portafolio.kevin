{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/_astro/(.*)",
      "dest": "/_astro/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
