{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/_astro/(.*)",
      "dest": "/_astro/$1"
    },
    {
      "src": "/proyectos",
      "dest": "/proyectos/index.html"
    },
    {
      "src": "/sobremi", 
      "dest": "/sobremi/index.html"
    },
    {
      "src": "/testimonios",
      "dest": "/testimonios/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
